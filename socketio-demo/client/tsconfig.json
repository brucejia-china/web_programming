{
    "compilerOptions": {
      "module": "es2015",
      "target": "es2015",
      "sourceMap": true,
      "inlineSources": true,
      "declaration": false,
      "moduleResolution": "node",
      "noEmitOnError": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": false,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "strictNullChecks": false,
      "experimentalDecorators": true,
      "types": []
    }
  }
  